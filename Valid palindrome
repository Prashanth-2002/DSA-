class Main {
  public static void main(String[] args) {

      String str = "aabbbbaa"; 
      String reverse = "";
   
  
     
        
         for (int i = (str.length() - 1); i >=0; --i) {
      reverse = reverse + str.charAt(i);
    }
        if(str.toLowerCase().equals(reverse.toLowerCase()))
            System.out.print("Yes");
        
        else
        System.out.print("No");
    
        
    }
}
