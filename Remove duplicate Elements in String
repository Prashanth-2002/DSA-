import java.util.*;
class Main{
     static  String duplicate(String s){
    char[] arr    = s.toCharArray();
    String ans ="  ";
    for(char x : arr){
        if(ans.indexOf(x)==-1)
        ans=ans+x;
    }
    
        
    return ans;
     }
     
     public static void main(String[] args) {
     String s = "aabbcdd";
     System.out.print(duplicate(s));
     }
}
