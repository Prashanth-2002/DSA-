import java.util.*;

class Main{

 public static int max(int arr[]){
 int max=Integer.MIN_VALUE;
 int cur=0;
 for(int i=0;i<arr.length;i++){
 cur=cur+arr[i];
 if(cur>max)
 max=cur;
 
 if(cur<0)
 cur=0;
 
 }
 return max;
 }
 
 public static void main (String[] args)
    {
        int [] arr = {-2, -3, 4, -1, -2, 1, 5, -3};
        System.out.println("Maximum contiguous sum is " +
                                       max(arr));
    }
    
    
    }
 
 
