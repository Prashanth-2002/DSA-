class Solution{
    static int middle(int A, int B, int C){
        //code here
        if(A>B){
            if(A<C)
            return A;
           else  if(B>C)
          return B;
          else
          return C;
          
            
         }
        else {
            if(C>B)
            return B;
           else  if(A>C)
          return A;
          else
          return C;
          
            
        }
        
        
    
   
    }
    
    public static void main(String[] args)
    {
        int a = 40, b = 50, c = 45;
        System.out.println(middle(a, b, c));
    }
    
    
    
    
    
}
