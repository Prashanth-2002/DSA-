import java.util.*;

public class Main{

public static int findnum(int arr[],int n){


      int sum = 0;
        for (int i = 0; i < n-1; i++){
        sum = sum +arr[i];
        }
        
        
    int ogsum = n*(n+1)/2;  
    return(ogsum-sum);


public static void main(String[] args)
    {
        int[] arr = { 1, 2, 3, 5 };
        int n = arr.length;
        System.out.println(findnum(arr, n));
    }
}
